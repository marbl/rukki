{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build - graph_analysis"
		},
		{
			"type": "shell",
			"command": "cargo",
			"args": [
				"build",
				"--tests"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "Rust: cargo build --tests - graph_analysis"
		},
		{
			"type": "cargo",
			"subcommand": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "test",
			"label": "Rust: cargo test - graph_analysis"
		}
	]
}